(background (color-rgb 0 0 0))
(pattern "Wood #1")
(brush-size 51)
(brush-hardness 0.5)
(pattern-view-type list)
(fill-mode bg)
(fill-area selection)
(sample-merged yes)
(diagonal-neighbors yes)

